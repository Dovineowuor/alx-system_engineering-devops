# #!/usr/bin/env bash
# # gives you information about the holbertonschool file
#!/usr/bin/env bash
# check if file exists or not

if [ -e "codingschoolschool" ]; then
   echo "codingschoolschool file exists"
   if [ -s "codingschoolschool" ]; then
       echo "codingschoolschool file is not empty"
   else
       echo "codingschoolschool file is empty"
   fi
   if [ -f "codingschoolschool" ]; then
       echo "codingschoolschool is a regular file"
   fi
else
   echo "codingschoolschool file does not exist"
fi
